{
  "dashboard": {
    "id": null,
    "title": "Quantum Trading Matrix™ - System Monitoring",
    "description": "Comprehensive monitoring dashboard for the Quantum Trading Matrix™ platform",
    "tags": ["quantum-trading", "trading", "monitoring", "financial"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "5s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "System Overview",
        "type": "stat",
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 0,
          "y": 0
        },
        "targets": [
          {
            "expr": "trading_api_requests_total",
            "legendFormat": "Total API Requests",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 1000
                },
                {
                  "color": "red",
                  "value": 5000
                }
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "Response Time",
        "type": "stat",
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 6,
          "y": 0
        },
        "targets": [
          {
            "expr": "rate(trading_api_request_duration_seconds_sum[5m]) / rate(trading_api_request_duration_seconds_count[5m])",
            "legendFormat": "Avg Response Time",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 0.5
                },
                {
                  "color": "red",
                  "value": 1.0
                }
              ]
            }
          }
        }
      },
      {
        "id": 3,
        "title": "Trading Pods Status",
        "type": "stat",
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 12,
          "y": 0
        },
        "targets": [
          {
            "expr": "trading_pods_active_total",
            "legendFormat": "Active Pods",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 1
                },
                {
                  "color": "green",
                  "value": 3
                }
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "title": "Portfolio Value",
        "type": "stat",
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 18,
          "y": 0
        },
        "targets": [
          {
            "expr": "portfolio_total_value",
            "legendFormat": "Total Portfolio Value",
            "refId": "D"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "currencyUSD",
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                }
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "title": "API Request Rate",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 4
        },
        "targets": [
          {
            "expr": "rate(trading_api_requests_total[5m])",
            "legendFormat": "Requests/sec",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Requests per second",
            "min": 0
          }
        ],
        "legend": {
          "show": true
        }
      },
      {
        "id": 6,
        "title": "System Resources",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 4
        },
        "targets": [
          {
            "expr": "process_cpu_seconds_total",
            "legendFormat": "CPU Usage",
            "refId": "A"
          },
          {
            "expr": "process_resident_memory_bytes / 1024 / 1024",
            "legendFormat": "Memory Usage (MB)",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "label": "Usage",
            "min": 0
          }
        ]
      },
      {
        "id": 7,
        "title": "Trading Performance",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 12
        },
        "targets": [
          {
            "expr": "trading_profit_loss_total",
            "legendFormat": "P&L",
            "refId": "A"
          },
          {
            "expr": "trading_trades_executed_total",
            "legendFormat": "Trades Executed",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "label": "Amount",
            "min": 0
          }
        ]
      },
      {
        "id": 8,
        "title": "Options Analysis",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 12
        },
        "targets": [
          {
            "expr": "options_calculations_total",
            "legendFormat": "Options Calculations",
            "refId": "A"
          },
          {
            "expr": "options_portfolio_delta",
            "legendFormat": "Portfolio Delta",
            "refId": "B"
          },
          {
            "expr": "options_portfolio_gamma",
            "legendFormat": "Portfolio Gamma",
            "refId": "C"
          }
        ],
        "yAxes": [
          {
            "label": "Value",
            "min": null
          }
        ]
      },
      {
        "id": 9,
        "title": "Error Rate",
        "type": "graph",
        "gridPos": {
          "h": 6,
          "w": 12,
          "x": 0,
          "y": 20
        },
        "targets": [
          {
            "expr": "rate(trading_api_errors_total[5m])",
            "legendFormat": "Error Rate",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Errors per second",
            "min": 0
          }
        ],
        "alert": {
          "conditions": [
            {
              "evaluator": {
                "params": [0.1],
                "type": "gt"
              },
              "operator": {
                "type": "and"
              },
              "query": {
                "params": ["A", "5m", "now"]
              },
              "reducer": {
                "type": "avg"
              },
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "for": "1m",
          "frequency": "10s",
          "handler": 1,
          "name": "High Error Rate Alert",
          "noDataState": "no_data",
          "notifications": []
        }
      },
      {
        "id": 10,
        "title": "Database Connections",
        "type": "graph",
        "gridPos": {
          "h": 6,
          "w": 12,
          "x": 12,
          "y": 20
        },
        "targets": [
          {
            "expr": "database_connections_active",
            "legendFormat": "Active Connections",
            "refId": "A"
          },
          {
            "expr": "database_connections_idle",
            "legendFormat": "Idle Connections",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "label": "Connections",
            "min": 0
          }
        ]
      },
      {
        "id": 11,
        "title": "Quantum Neural Network Performance",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 0,
          "y": 26
        },
        "targets": [
          {
            "expr": "quantum_predictions_accuracy",
            "legendFormat": "Prediction Accuracy",
            "refId": "A"
          },
          {
            "expr": "quantum_processing_time",
            "legendFormat": "Processing Time",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "label": "Performance",
            "min": 0,
            "max": 1
          }
        ]
      },
      {
        "id": 12,
        "title": "ESG Scores",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 8,
          "y": 26
        },
        "targets": [
          {
            "expr": "esg_environmental_score",
            "legendFormat": "Environmental",
            "refId": "A"
          },
          {
            "expr": "esg_social_score",
            "legendFormat": "Social",
            "refId": "B"
          },
          {
            "expr": "esg_governance_score",
            "legendFormat": "Governance",
            "refId": "C"
          }
        ],
        "yAxes": [
          {
            "label": "Score",
            "min": 0,
            "max": 100
          }
        ]
      },
      {
        "id": 13,
        "title": "Compliance Status",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 16,
          "y": 26
        },
        "targets": [
          {
            "expr": "compliance_checks_passed_total",
            "legendFormat": "Checks Passed",
            "refId": "A"
          },
          {
            "expr": "compliance_checks_failed_total",
            "legendFormat": "Checks Failed",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "label": "Count",
            "min": 0
          }
        ]
      }
    ],
    "annotations": {
      "list": [
        {
          "builtIn": 1,
          "datasource": "-- Grafana --",
          "enable": true,
          "hide": true,
          "iconColor": "rgba(0, 211, 255, 1)",
          "name": "Annotations & Alerts",
          "type": "dashboard"
        }
      ]
    },
    "editable": true,
    "gnetId": null,
    "graphTooltip": 0,
    "links": [],
    "schemaVersion": 27,
    "version": 1
  }
} 