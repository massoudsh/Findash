{
  "dashboard": {
    "title": "Kafka + Redis + Celery Integration",
    "tags": ["kafka", "redis", "celery", "integration", "streaming"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 1,
    "refresh": "5s",
    "panels": [
      {
        "id": 1,
        "title": "Kafka Message Throughput",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "rate(kafka_messages_produced_total[5m])",
            "legendFormat": "{{symbol}} - Produced",
            "refId": "A"
          },
          {
            "expr": "rate(kafka_messages_consumed_total[5m])",
            "legendFormat": "{{symbol}} - Consumed",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "Messages/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 2,
        "title": "Kafka Produce/Consume Latency",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(kafka_produce_latency_seconds_bucket[5m]))",
            "legendFormat": "{{topic}} - Produce p95",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(kafka_consume_latency_seconds_bucket[5m]))",
            "legendFormat": "{{topic}} - Consume p95",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Latency"},
          {"format": "short"}
        ]
      },
      {
        "id": 3,
        "title": "Redis Cache Performance",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "rate(redis_cache_hits_total[5m])",
            "legendFormat": "{{cache_type}} - Hits",
            "refId": "A"
          },
          {
            "expr": "rate(redis_cache_misses_total[5m])",
            "legendFormat": "{{cache_type}} - Misses",
            "refId": "B"
          },
          {
            "expr": "rate(redis_cache_hits_total[5m]) / (rate(redis_cache_hits_total[5m]) + rate(redis_cache_misses_total[5m]))",
            "legendFormat": "Hit Rate",
            "refId": "C"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Count"},
          {"format": "percentunit", "label": "Hit Rate"}
        ]
      },
      {
        "id": 4,
        "title": "Redis Pub/Sub Messages",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "rate(redis_pubsub_messages_total[5m])",
            "legendFormat": "{{channel}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "Messages/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 5,
        "title": "Market Data Updates",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "rate(market_data_updates_total[5m])",
            "legendFormat": "{{symbol}} - {{exchange}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "Updates/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 6,
        "title": "Market Data Processing Lag",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
        "targets": [
          {
            "expr": "market_data_lag_seconds",
            "legendFormat": "{{symbol}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Lag (seconds)"},
          {"format": "short"}
        ],
        "alert": {
          "conditions": [
            {
              "evaluator": {
                "params": [5],
                "type": "gt"
              },
              "operator": {
                "type": "and"
              },
              "query": {
                "params": ["A", "5m", "now"]
              },
              "reducer": {
                "params": [],
                "type": "last"
              },
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "for": "2m",
          "frequency": "10s",
          "handler": 1,
          "name": "High Processing Lag",
          "noDataState": "no_data",
          "notifications": []
        }
      },
      {
        "id": 7,
        "title": "Celery Tasks from Kafka Stream",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24},
        "targets": [
          {
            "expr": "rate(celery_task_started_total{task_name=\"update_market_data\"}[5m])",
            "legendFormat": "Tasks Started",
            "refId": "A"
          },
          {
            "expr": "rate(celery_task_succeeded_total{task_name=\"update_market_data\"}[5m])",
            "legendFormat": "Tasks Succeeded",
            "refId": "B"
          },
          {
            "expr": "rate(celery_task_failed_total{task_name=\"update_market_data\"}[5m])",
            "legendFormat": "Tasks Failed",
            "refId": "C"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "Tasks/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 8,
        "title": "End-to-End Pipeline Flow",
        "type": "stat",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24},
        "targets": [
          {
            "expr": "sum(rate(kafka_messages_produced_total[5m]))",
            "refId": "A",
            "format": "table"
          },
          {
            "expr": "sum(rate(kafka_messages_consumed_total[5m]))",
            "refId": "B"
          },
          {
            "expr": "sum(rate(redis_pubsub_messages_total[5m]))",
            "refId": "C"
          },
          {
            "expr": "sum(rate(celery_task_succeeded_total{task_name=\"update_market_data\"}[5m]))",
            "refId": "D"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 1, "color": "yellow"},
                {"value": 5, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 9,
        "title": "Pipeline Health Status",
        "type": "table",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 32},
        "targets": [
          {
            "expr": "kafka_messages_produced_total",
            "format": "table",
            "refId": "A"
          },
          {
            "expr": "kafka_messages_consumed_total",
            "format": "table",
            "refId": "B"
          },
          {
            "expr": "redis_pubsub_messages_total",
            "format": "table",
            "refId": "C"
          },
          {
            "expr": "celery_task_succeeded_total{task_name=\"update_market_data\"}",
            "format": "table",
            "refId": "D"
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {},
              "indexByName": {},
              "renameByName": {
                "Value": "Count",
                "topic": "Kafka Topic",
                "symbol": "Symbol",
                "channel": "Redis Channel",
                "task_name": "Task"
              }
            }
          }
        ]
      }
    ],
    "time": {
      "from": "now-15m",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m"]
    }
  }
}

